<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="ring"
            android:thicknessRatio="5"
            android:useLevel="false">
            <solid android:color="#3c4993" />
        </shape>
    </item>
    <item>
        <rotate
            android:fromDegrees="270"
            android:toDegrees="270">
            <shape
                android:shape="ring"
                android:thicknessRatio="5"
                android:useLevel="true">
                <gradient
                    android:endColor="#6bbaea"
                    android:startColor="#33d69d"
                    android:type="linear"
                    android:angle="0"/>
            </shape>
        </rotate>
    </item>
</layer-list>